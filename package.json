{
    "name": "tomohauk",
    "executableName": "Tomohauk",
    "productName": "Tomohauk",
    "description": "Apache config electron",
    "version": "0.1.4",
    "license": "MIT",
    "author": {
        "name": "Shepel Pavel",
        "email": "pavel@shepel.net",
        "url": "https://shepel.net/"
    },
    "repository": "https://github.com/shepelpavel/apache-config-electron",
    "keywords": [
        "Apache",
        "config",
        "electron",
        "tomohauk"
    ],
    "main": "main.js",
    "scripts": {
        "clean": "rimraf dist",
        "start": "electron .",
        "build:64": "electron-packager . tomohauk --platform linux --arch x64 --out dist/ --overwrite",
        "deb:64": "electron-installer-debian --src dist/tomohauk-linux-x64/ --arch amd64 --config config.json"
    },
    "devDependencies": {
        "electron": "^10.1.0",
        "electron-installer-debian": "^3.1.0",
        "electron-is": "^3.0.0",
        "electron-packager": "^15.1.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^3.1.1",
        "gulp-cheerio": "^0.6.3",
        "gulp-cssnano": "^2.1.2",
        "gulp-imagemin": "^6.1.1",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^4.0.2",
        "gulp-svg-sprite": "^1.3.7",
        "gulp-svgmin": "^2.2.0",
        "gulp-terser": "^1.4.0",
        "gulp-watch": "^5.0.1",
        "node-sass": "^4.13.0",
        "pump": "^2.0.0"
    }
}
