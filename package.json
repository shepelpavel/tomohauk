{
    "name": "tomohauk",
    "executableName": "Tomohauk",
    "productName": "Tomohauk",
    "version": "0.0.5",
    "description": "Apache config electron",
    "main": "main.js",
    "scripts": {
        "start": "electron .",
        "build": "electron-packager . tomohauk --platform linux --arch x64 --out dist/ --overwrite --icon=icons/icon.png",
        "deb64": "electron-installer-debian --src dist/tomohauk-linux-x64/ --dest dist/installers/ --arch amd64"
    },
    "repository": "https://github.com/shepelpavel/apache-config-electron",
    "keywords": [
        "Apache",
        "config",
        "electron",
        "tomohauk"
    ],
    "author": "Shepel",
    "license": "MIT",
    "devDependencies": {
        "electron": "^10.1.0",
        "electron-installer-debian": "^3.1.0",
        "electron-is": "^3.0.0",
        "electron-packager": "^15.1.0"
    }
}
